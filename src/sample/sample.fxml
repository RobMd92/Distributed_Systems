<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<Pane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <GridPane alignment="center" hgap="10" vgap="10">
      
      
         <children>
            <ListView fx:id="List1" editable="true"  prefHeight="200.0" prefWidth="200.0" />
            <ListView fx:id="List2" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" />
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1">
               <children>
                  <Button fx:id="dBtn" alignment="TOP_CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#Dbtn" text="Download" />
                  <Button fx:id="uBtn" alignment="CENTER" mnemonicParsing="false" text="Upload" />
                  <Button fx:id="pBtn" mnemonicParsing="false" text="Play" />
               </children>
            </VBox>
         </children>
         <columnConstraints>
            <ColumnConstraints maxWidth="194.0" minWidth="168.0" prefWidth="168.0" />
            <ColumnConstraints maxWidth="67.0" minWidth="5.0" prefWidth="67.0" />
            <ColumnConstraints maxWidth="195.0" minWidth="165.0" prefWidth="165.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
         </rowConstraints>
      </GridPane>
   </children>
</Pane>
